<%= form_with(model: voice, local: true) do |form| %>
<% if voice.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(voice.errors.count, "error") %>
      prohibited this voice from being saved:</h2>

    <ul>
      <% voice.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %>

